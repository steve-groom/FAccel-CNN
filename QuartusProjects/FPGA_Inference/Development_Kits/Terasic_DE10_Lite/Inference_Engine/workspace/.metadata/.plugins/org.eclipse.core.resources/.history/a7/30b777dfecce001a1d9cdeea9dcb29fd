int main()
{ 
  alt_putstr("Hello from Nios II!\n");

  /* Event loop never exits. */
  while (1) {
	  IOWR_32DIRECT(LED_BASE, 0, 0);
	  usleep(250000);
	  IOWR_32DIRECT(LED_BASE, 0, 1);
	  usleep(250000);
  }

  return 0;
}
